class GfG { 
    public static void main(String args[]) 
    { 
        String str = "1234"; 
  
        int num1 = Integer.parseInt(str); 
        System.out.println("Integer using "
                           + "first variant of"
                           + " praseInt = "
                           + num1); 
  
        int num2 = Integer.parseInt(str, 16); 
        System.out.println("Integer using "
                           + "second (radix) variant"
                           + " of praseInt = "
                           + num2); 
    } 
} 
