#include<conio.h>
#include<iostream.h>
class item
{ int n,m;
 public:
 int largest(int x,int y);
 void display(int x,int y);
};
int item::largest(int x,int y)
{if(x>y)
    return x;
 else
    return y;
}
void item:: display(int x,int y)
{cout<<"First no.  =>"<<x<<endl;
cout<<"Second no. =>"<<y<<endl;
 cout<<"Largest =>"<<largest(x,y);
}
void main()
{
clrscr();
int x,y;
cout<<"Enter two no. =>"<<endl;
cin>>x>>y;

item I;

 I.display(x,y);
getch();
}
